# Production Environment Template
# Copy this file to .env.production and fill in your production values
# DO NOT commit .env.production with real credentials

# Firebase Configuration (from Firebase Console → Project Settings → Your Apps)
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id_here
VITE_FIREBASE_APP_ID=your_app_id_here
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id_here

# Access Control (comma-separated list of allowed user emails)
VITE_ALLOWED_EMAILS=user1@company.com,user2@company.com

# API Endpoint - Production Cloud Run backend
VITE_API_BASE_URL=https://your-backend-service-xxx.run.app
